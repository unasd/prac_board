<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0/EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- <settings>
		<setting name="logIml" value="LOG4J"/>
	</settings> -->
	<typeAliases>
		<typeAlias type="model.board.BoardModel" alias="boardModel"/>
		<typeAlias type="model.board.CommentModel" alias="commentModel"/>
		<typeAlias type="model.board.FileModel" alias="fileModel"/>
	</typeAliases>
	<environments default="development">
		<environment id="development">
		<transactionManager type="JDBC"/>
		<dataSource type="POOLED">
			<property name="driver" value="com.mysql.jdbc.Driver"/>
			<property name="url" value="jdbc:mysql://127.0.0.1:3306/jshboard"/>
			<property name="username" value="root"/>
			<property name="password" value="into20"/>
		</dataSource>
		</environment>
</environments>
	<mappers>
		<mapper resource="mybatis/sqlmap/board.xml"/>
	</mappers>
</configuration>